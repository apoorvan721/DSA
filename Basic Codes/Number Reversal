import java.util.*;
import java.lang.*;
import java.io.*;

class WorkAtTech
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		int rev=0;
		while(n!=0){
		int rem=n%10;
		rev=rem+rev*10;
		n=n/10;
		}
		System.out.print(rev);
	}
}
