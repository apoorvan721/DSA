import java.util.*;
import java.lang.*;
import java.io.*;

class WorkAtTech
{
	public static void main (String[] args) throws java.lang.Exception
	{
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		int a[]=new int[n];
		for(int i=0;i<n;i++){
			a[i]=sc.nextInt();
			if(a[i]>7){
				System.out.println("UP");
			}
			else if(a[i]<7){
				System.out.println("DOWN");
			}
			else{
				System.out.println("EQUAL");
			}
		}
	}
}
