class Solution {
    public void moveZeroes(int[] nums) {
        int size=0;
        for(int i=0;i<nums.length;i++){
            if(nums[i]!=0){
                nums[size]=nums[i];
                size++;
            }
        }
        while(size<nums.length){
            nums[size]=0;
            size++;
        }
    }
}
