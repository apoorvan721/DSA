class Solution {
    public void merge(int[] nums1, int m, int[] nums2, int n) {
        int index1=m;
        int index2=0;
            while(index1<m+n && index2<n){
                nums1[index1]=nums2[index2];
                index1++;
                index2++;
            }
            Arrays.sort(nums1);
    }
}

